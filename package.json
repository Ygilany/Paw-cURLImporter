{
  "name": "Paw-cURLImporter",
  "version": "2.0.0",
  "main": "extention.js",
  "license": "MIT",
  "homepage": "https://github.com/luckymarmot/Paw-cURLImporter",
  "dependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-polyfill": "^6.3.14",
    "babel-runtime": "^6.3.19",
    "immutable": "^3.7.6"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-rewire": "^1.0.0-beta-3",
    "babel-plugin-transform-decorators": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "path": "^0.12.7",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "upgrade": "npm-check-updates -u && npm test",
    "test": "$npm_package_options_mocha \"src/**/__tests__/*-test.js\"",
    "test:watch": "npm test -- --watch",
    "prestart": "npm install",
    "build": "rm -rf dist/ && BUILD_ENV=build ./node_modules/.bin/webpack --bail --display-error-details",
    "dist": "npm install --production && NODE_ENV=production npm run build",
    "lint": "./node_modules/eslint/bin/eslint.js -c linting/dev.yaml src/"
  },
  "options": {
    "mocha": "./node_modules/.bin/mocha --require mocha --compilers js:babel-register --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:luckymarmot/Paw-cURLImporter.git"
  }
}
